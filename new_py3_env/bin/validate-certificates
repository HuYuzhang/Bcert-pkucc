#!/Users/jasmine/Documents/Bcert-pkucc/new_py3_env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'blockchain-certificates==2.1.6','console_scripts','validate-certificates'
__requires__ = 'blockchain-certificates==2.1.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('blockchain-certificates==2.1.6', 'console_scripts', 'validate-certificates')()
    )
